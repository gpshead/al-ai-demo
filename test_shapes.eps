% Draw a filled triangle
newpath
50 50 moveto
150 50 lineto
100 150 lineto
closepath
0.7 setgray
fill

% Draw a rectangle with thick outline
newpath
30 30 moveto
170 30 lineto
170 80 lineto
30 80 lineto
closepath
0 setgray
5 setlinewidth
stroke

% Draw a diamond shape
newpath
100 20 moveto
180 100 lineto
100 180 lineto
20 100 lineto
closepath
0.3 setgray
fill

% Draw crossing lines
newpath
10 100 moveto
190 100 lineto
0.8 setgray
2 setlinewidth
stroke

newpath
100 10 moveto
100 190 lineto
0.4 setgray
3 setlinewidth
stroke